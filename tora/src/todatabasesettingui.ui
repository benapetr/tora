    <tabstop>ReadAll</tabstop>
    <tabstop>MaxContent</tabstop>
    <tabstop>UnlimitedContent</tabstop>
    <tabstop>MaxColDisp</tabstop>
    <tabstop>IndicateEmpty</tabstop>
    <tabstop>NumberFormat</tabstop>
