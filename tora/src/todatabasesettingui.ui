                    </property>
                    <property name="text">
                        <string>Indicate empty values as '' instead of NULL as {null}.</string>
                    </property>
                </widget>
                <widget class="QComboBox" row="1" column="1">
                    <item>
                        <property name="text">
                            <string>Default</string>
